{
  "name": "extension-monorepo",
  "version": "0.0.1",
  "description": "no",
  "author": "yasin TAZEOGLU <yasinntazeoglu@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build-webpack":"node webpack/index.js",
    "build-packagejson":"node webpack/index.js",
    "build": "yarn lint && yarn tsc-check && rm -rf bundle && yarn build-webpack  && node webpack.js && node starter.js && rm -rf webpack.js starter.js",
    "start": "cd bundle && yarn start",
    "start-watch": "nodemon --ext js,html --watch bundle --exec 'yarn start'",
    "build-watch": "nodemon --ignore **/dist/**  --ext js,ts --watch template --watch extension --exec 'yarn build'",
    "lint": "eslint --fix extension/**/*.ts --cache",
    "tsc-check": "cd extension && tsc --noEmit"
  },
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/lowdb": "^1.0.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^12.0.0",
    "@types/node-fetch": "^2.3.3",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "add-module-exports-webpack-plugin": "^1.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "log-remote": "^0.0.1",
    "lowdb": "^1.0.0",
    "nodemon": "^1.19.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-feather": "^1.1.6",
    "style-loader": "^0.23.1",
    "styled-jsx": "^3.2.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.1.0",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.4.5",
    "vscode": "^1.1.33",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-prettier": "^3.1.0",
